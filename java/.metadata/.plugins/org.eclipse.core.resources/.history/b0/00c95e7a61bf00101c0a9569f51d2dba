import java.util.*;
import java.math.*;
class test{
	public static void main(String[] args)
	{
		int n;
		Scanner cin = new Scanner(System.in);
		BigInteger a,b;
		n = cin.nextInt();
		for(int cs = 1;cs <= n;cs++)
		{
			System.out.println("Case "+cs+":");
			a = cin.nextBigInteger();
			b = cin.nextBigInteger();
			System.out.println(a+" + "+b+" = "+a.add(b));
		}
	}
}
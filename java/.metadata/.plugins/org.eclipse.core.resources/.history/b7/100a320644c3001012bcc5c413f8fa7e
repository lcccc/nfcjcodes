import java.math.BigDecimal;
import java.util.Scanner;
public class big4 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner cin = new Scanner(System.in);
		BigDecimal x;
		int n;
		while(cin.hasNext())
		{
			x = cin.nextBigDecimal();
			n = cin.nextInt();
			x = x.pow(n);
			System.out.println(x.toString());
		}
	}

}

import java.math.BigInteger;
import java.util.*;

public class test {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner cin = new Scanner(System.in);
		String s,a = null,b = null;
		int n;
		BigInteger ba,bb;
		n = cin.nextInt();
		for(int i = 0;i<n;i++){
		s = cin.next();
		String[] arr = new String[2];
		arr = s.split(" ");
		a += arr[0];
		b += arr[1];
		}
		ba = new BigInteger(a);
		bb = new BigInteger(b);
		System.out.println(ba.add(bb));
	}

}

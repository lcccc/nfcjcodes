import java.math.*;
import java.util.Scanner;
import java.io.*;
public class C {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		BigInteger[][] f = new BigInteger[252][252];
		for(int i=0; i<=250;i++)
			for(int j=0;j<=250;j++)
				f[i][j] = new BigInteger("0");
		try{
		Scanner cin = new Scanner(System.in);
		while(cin.hasNext()){
		//BigInteger[][] f = new BigInteger[252][252];
		int n,k;
		n = cin.nextInt();
		k = cin.nextInt();
		//System.out.println(n);
		
		f[1][1] = new BigInteger("1");
		
		for(int i = 2;i<= n;i++ )
			for(int j = 1; j<=i;j++)
			{
				if( j-1 <=0) f[i][j] = f[i-1][j];
				
				else {
					if(j>i-1) f[i][j] = f[i-1][j-1];
					else
					f[i][j] = f[i-1][j].add(f[i-1][j-1]);
				}
				//System.out.print(i);
				//System.out.print(" ");
				//System.out.print(j);
				//System.out.println( f[i][j]);
				}
		BigInteger fm = new BigInteger("1");
		for(int i = 1; i<=n;i++)
			fm = fm.multiply(new BigInteger(Integer.toString(i)));
		BigInteger big = fm.gcd(f[n][k]);
		//System.out.println(fm);
		//System.out.print(" ");
		//System.out.println(big);
		System.out.print(f[n][k].divide(big));
		System.out.print("/");
		System.out.println(fm.divide(big));
		}
		}catch(Exception e)
		{
			return ;
		}
		}
	}

